<div class="form-group">
	<%= f.text_field :name, class: "form-control", id: "exampleInputChildrens", placeholder: "Children Name", required: true %>
	<%= f.hidden_field :_destroy %>
	<%= link_to_function "Remove", '$(this).closest("form").find("#exampleInputChildrens").next().val(1);$(this).closest("form").find("#exampleInputChildrens").remove();$(this).remove();', class: "remove_fields" %>
</div>