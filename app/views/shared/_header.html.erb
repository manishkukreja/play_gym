<div class="navbar-wrapper">
  <div class="container">
    <div class="navbar navbar-inverse navbar-static-top" role="navigation">
      <div class="container">
        <div class="navbar-header">
          <%= link_to image_tag('playgym_logo.png' ,:size => '118x50', style: "padding-right: 5px;") , :root%>
          <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
        </div>
        <div class="navbar-collapse collapse">
          <ul class="nav navbar-nav">
            <li class='<%= "active" if controller.controller_name == "memberships" %>'><%= link_to "Memberships", memberships_path, class: "color1" %></li>
            <li class='<%= "active" if controller.controller_name == "activities" && controller.action_name == "index" %>'><%= link_to "Programs & Clases", activities_path, class: "color2" %></li>
            <li class='<%= "active" if controller.action_name == "events_list" %>'><%= link_to "Events & Parties", events_list_activities_path, class: "color3" %></li>
            <li class='<%= "active" if controller.action_name == "coming_soon" %>'><%= link_to "Social", coming_soon_home_index_path, class: "color4" %></li>
            <li class='<%= "active" if controller.action_name == "coming_soon1" %>'><%= link_to "Our Partners", coming_soon_home_index_path, class: "color5" %></li>
            <li class='<%= "active" if controller.action_name == "coming_soon2" %>'><%= link_to "About Us", coming_soon_home_index_path, class: "color6" %></li>
            <li class='<%= "active" if controller.controller_name == "sessions" && controller.action_name == "new" %>'>
              <% if user_signed_in? %>
                <%= link_to "Admin Logout", destroy_user_session_path, method: :delete, class: "color7" %>
              <% else %>
                <%= link_to "Admin Login", new_user_session_path, class: "color7" %>
              <% end %>
            </li>
          </ul>          
        </div>
      </div>
    </div>    
    <%= render "/shared/social_sdks" %>
  </div>
</div>
